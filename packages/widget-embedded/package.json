{
  "name": "@lifi/widget-embedded",
  "version": "1.0.220",
  "type": "module",
  "scripts": {
    "analyze": "source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "dev": "vite",
    "build": "tsc && vite build",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "preview": "vite preview",
    "lint": "eslint --ext .tsx --ext .ts ./src",
    "format": "prettier --write ./src/",
    "pre-commit:validate": "yarn format && yarn lint --fix --max-warnings=0",
    "pre-push:validate": "yarn check:types"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Eugene Chybisov <eugene@li.finance>",
  "dependencies": {
    "@lifi/sdk": "^3.2.0",
    "@lifi/wallet-management": "^3.1.5-beta.0",
    "@lifi/widget": "^3.5.2",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@opensea/seaport-js": "4.0.4",
    "@tanstack/react-query": "^5.55.4",
    "bignumber.js": "^9.1.2",
    "ethers": "^6.13.2",
    "events": "^3.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "wagmi": "^2.12.9"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.3",
    "vite-plugin-node-polyfills": "^0.22.0",
    "web-vitals": "^4.2.3"
  },
  "eslintConfig": {
    "extends": "../../.eslintrc"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not dead",
      "not IE > 0",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "private": true
}
