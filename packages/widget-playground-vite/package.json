  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-plugin-node-polyfills": "0.24.0",
    "web-vitals": "^5.1.0"
